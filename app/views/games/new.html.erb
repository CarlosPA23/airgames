<h1>Oh! Do you have a new game pal?</h1>
<%= simple_form_for (@game) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :category %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Add your game", class: "btn btn-primary" %>
<% end %>
<%= link_to "Back", games_path %>
